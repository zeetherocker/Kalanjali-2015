<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zap.Kalanjali.Schedule">

    <com.zap.Kalanjali.ZoomImageView
        android:id="@+id/image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/sched"
        custom:allowExcedLimitsWhenMovingImage="false"
        custom:blockImageInTheMidleWhileIsSmallerThanView="true"
        custom:doubleClickTimeInMillis="275"
        custom:mininumZoomLevel="0.75"
        custom:doubleClickZoomLevel="3"
        custom:maximumZoomLevel="5"
        />

</FrameLayout>
